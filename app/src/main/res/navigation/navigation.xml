<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/contactsTitleFragment2">
    <fragment
        android:id="@+id/contactsTitleFragment2"
        android:name="com.example.mycontacts.contactstitleFragment.contactsTitleFragment"
        android:label="contacts"
        tools:layout="@layout/fragment_contacts_title">
        <action
            android:id="@+id/action_contactsTitleFragment2_to_contactsEditor"
            app:destination="@id/contactsEditor" />
        <action
            android:id="@+id/action_contactsTitleFragment2_to_updateContacts"
            app:destination="@id/updateContacts" />

        />
    </fragment>
    <fragment
        android:id="@+id/contactsEditor"
        android:name="com.example.mycontacts.contactsEditor.contactsEditor"
        android:label="contacts"
        tools:layout="@layout/fragment_contacts_editor">
        <action
            android:id="@+id/action_contactsEditor_to_contactsTitleFragment2"
            app:destination="@id/contactsTitleFragment2"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/updateContacts"
        android:name="com.example.mycontacts.updateContacts.updateContacts"
        android:label="updateContacts"
        tools:layout="@layout/update">
        <action
            android:id="@+id/action_updateContacts_to_contactsTitleFragment2"
            app:destination="@id/contactsTitleFragment2"
            app:launchSingleTop="false"
            app:popUpTo="@id/updateContacts"

            app:popUpToInclusive="true" />
        <argument
            android:name="contactsKey"
            app:argType="long" />
    </fragment>
</navigation>